{
  "extends": "soywod-tslint-rules",
  "rules": {
    "no-bitwise": false,
    "object-literal-sort-keys": [
      true,
      "ignore-case",
      "locale-compare",
      "match-declaration-order",
      "shorthand-first"
    ],
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "always",
          "typeLiterals": "never"
        }
      }
    ]
  }
}
