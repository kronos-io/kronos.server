{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "plugins": [{
      "name": "tslint-language-service"
    }],
    "sourceMap": true,
    "strict": true,
    "target": "ES6",
    "typeRoots": ["node_modules/@types", "src/types"]
  },
  "exclude": [
    "node_modules"
  ]
}
